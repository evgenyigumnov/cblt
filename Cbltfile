"*:80" {
    reverse_proxy "/http/*" "http://127.0.0.1:8080" "http://127.0.0.1:8081"{
       health_uri "/health"
       health_interval "10s"
       health_timeout "2s"
       lb_policy "cookie" {
           lb_cookie_name "my_sticky_cookie"
           lb_cookie_path "/"
           lb_cookie_max_age "3600"
       }
    }
    root "*" "./assets"
    file_server
}
"*:443" {
    root "*" "./assets"
    file_server
    tls "domain.crt" "domain.key"
}
